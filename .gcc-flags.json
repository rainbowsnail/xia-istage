{
	"execPath": "/usr/bin/g++",
	"gccDefaultCFlags": "-c -Wall -fsyntax-only",
	"gccDefaultCppFlags": "-c -Wall -std=c++11 -fsyntax-only",
	"gccErrorLimit": -1,
	"gccIncludePaths": ".,./include,./path",
	"gccIncludePaths": "./api/*,./click/*,./daemons/*",
	"gccLintOnTheFly": true,
	"gccSuppressWarnings": false
}
